{
	"name": "aw-studio/laravel-migrations-merger",
	"license": "MIT",
	"authors": [
		{
			"name": "Lennart Carstens-Behrens",
			"email": "lennart.carbe@gmail.com"
		}
	],
	"require": {
		"illuminate/database": "^7.0|^8.0",
		"illuminate/filesystem": "^7.0|^8.0",
		"illuminate/support": "^7.0|^8.0"
	},
	"require-dev": {
		"orchestra/testbench": "^5.0|^6.0"
	},
	"autoload": {
		"psr-4": {
			"AwStudio\\MigrationsMerger\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"AwStudio\\MigrationsMerger\\MigrationsMergerServiceProvider"
			]
		},
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
